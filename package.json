{
  "name": "@stereobooster/cookie",
  "description": "Tiny cookie component",
  "version": "1.1.4",
  "license": "MIT",
  "devDependencies": {
    "chai": "^4.2.0",
    "microbundle": "^0.11.0",
    "mocha": "^6.1.4",
    "mocha-headless-chrome": "^2.0.2",
    "npm-run-all": "^4.1.5",
    "serve": "^11.0.1",
    "size-limit": "^1.3.6"
  },
  "files": [
    "index.js"
  ],
  "source": "index.js",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "unpkg": "dist/index.umd.js",
  "amdName": "cookie",
  "scripts": {
    "build": "microbundle",
    "build:watch": "microbundle watch",
    "serve": "serve .",
    "start": "run-p serve build:watch",
    "test": "yarn mocha-headless-chrome -f http://localhost:5000/test",
    "size": "yarn build && size-limit",
    "prePublish": "yarn build"
  },
  "size-limit": [
    {
      "limit": "550b",
      "path": "dist/*.{mjs,js}"
    }
  ]
}
